## This is a generic .gitignore, some of these are just autogenerated and not used
# Ignore node_modules directory
node_modules/

# Ignore npm lock files
package-lock.json
yarn.lock

# Ignore .env files containing sensitive information
.env

# Ignore logs and generated files
logs/
dist/
coverage/

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore user-specific files
.vscode/
.idea/

# Ignore editor configuration files
.editorconfig
.eslintrc
.prettierrc

# ignore database files
machineState.json
rbm_nodes.json

